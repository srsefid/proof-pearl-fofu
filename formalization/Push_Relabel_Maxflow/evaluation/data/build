#!/bin/bash
gcc data-gen.c -o data-gen

mkdir -p samples
cd samples

test -f g-100-D.txt || ../data-gen 100 .25 D 1.4

for ((i=1000;$i<=1450;i+=50)); do
  test -f g-$i-D.txt || ../data-gen $i .25 D 1.4
done

for ((i=1000;$i<=5500;i+=500)); do
  test -f g-$i-S.txt || ../data-gen $i .02 S 1
done

cd ..
